<div class="mb-8">
  <%= render 'form' %>
</div>
<%# <div class="empty"> %>
<%= turbo_frame_tag Tweet.new do %>
  <% if @tweets.none? %>
    <%= render "empty"%>
  <% end %>
<% end %>
<%# </div> %>
<%= turbo_frame_tag 'tweets', class: "divide-y list-none" do %>
  <% @tweets.each do |tweet| %>
    <%= render partial: "tweets/#{tweet.tweet_type.parameterize}", locals: {tweet: tweet} %>
  <% end %>
<% end %>
